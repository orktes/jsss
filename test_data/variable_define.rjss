@var width: 100px;
@var height: 100px;
@var square: ${parseInt(height) * parseInt(width)};
@func calc (attr1, attr2) {
  return attr1 * attr2;
}

@func funwithstatement (foo) {
  var bar = "bar";
  var fooval = 123;
  if (true || 1 == 1) {
    return fooval + foo;
  } else {
    return bar;
  }
}

main {
  top: 10;
  left: 0;
  width: ${width};
  square: $square;
  height: ${(function () { return height; })()};
  line-height: calc(20, 2);
  fontSize: 12;
  advanced: funwithstatement("bar");
}
