@var width: 100px;
@var height: 100px;
@func calc (attr1, attr2) {
  return attr1 * attr2;
}

@func funwithstatement (foo) {
  if (true) {
    return "foo";
  } else {
    return "bar";
  }
}

main {
  top: 10;
  left: 0;
  width: ${width};
  height: ${height};
  line-height: calc(20, 2);
  fontSize: 12;
}
